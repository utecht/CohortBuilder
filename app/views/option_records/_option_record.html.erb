<div id="<%= dom_id option_record %>">
  <p>
    <strong>Type:</strong>
    <%= option_record.type %>
  </p>

  <p>
    <strong>Patient ID:</strong>
    <%= option_record.patient_id %>
  </p>

  <p>
    <strong>Field:</strong>
    <%= option_record.field.name %>
  </p>

  <p>
    <strong>Option value:</strong>
    <%= option_record.option.value %>
  </p>

  <p>
    <strong>String value:</strong>
    <%= option_record.string_value %>
  </p>

</div>
