<div id="<%= dom_id field_group %>">
  <p>
    <strong>Name:</strong>
    <%= field_group.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= field_group.description %>
  </p>

  <p>
    <strong>Document:</strong>
    <%= field_group.document_id %>
  </p>

  <p>
    <strong>Fields:</strong>
  </p>
  <% field_group.fields.each do |field| %>
  <p><%= link_to field.name, field %></p>
  <% end %>

</div>
